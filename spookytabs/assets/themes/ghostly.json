{
  "name": "Ghostly Minimalist",
  "description": "Ethereal whites and misty grays with floating particles",
  "colors": {
    "primary": "#0a0a0a",
    "secondary": "#1a1a1a",
    "accent": "#e8e8e8",
    "highlight": "#ffffff",
    "glow": "rgba(232, 232, 232, 0.3)"
  },
  "fonts": {
    "primary": "'Courier New', monospace",
    "accent": "'Georgia', serif"
  },
  "particles": {
    "type": "floating",
    "color": "#ffffff",
    "opacity": 0.3,
    "count": 30
  },
  "effects": {
    "vignette": true,
    "cursorTrail": true,
    "screenShake": false
  }
}
